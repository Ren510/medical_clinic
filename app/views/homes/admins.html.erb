<br>
	<div class="container">
        <div class="row">
            <div class="col-md-1"></div>
                <p></p>
                <h1><strong><i class="fas fa-user-md"></i>医師紹介</strong></h1>
            </div>
        </div>
    </div>
    <p></p>
    <div class="container text-center">
        <div class="row">
			<section class="inner">
			  <% if flash[:notice] %>
			    <p class="text-active"><%= flash[:notice] %></p>
			  <% end %>
			</div>
			<table class="table table-striped table-dark">
                <th scope="col"></th>
                <td><% AdminUser.all.each do |au| %>
                    <% if au.image_id.present? %>
                        <%= image_tag attachment_url(au, :image, :fill, 200, 200)  %>
                    <% end %>
                    
                    <tr><td><% if au.image_id.present? %>
                        <%= image_tag attachment_url(au, :image, :fill, 200, 200)  %>
                    <% end %></td></tr>
                    <tr><td><%= au.name %></td></tr>
                    <tr><td><%= au.introduction %></td></tr>
                    <% end %>
                </td>
			</table><br>
		</div>
	</div>
    